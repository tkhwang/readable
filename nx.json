{
  "npmScope": "readable",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e", "build-storybook"]
      }
    }
  },
  "projects": {
    "client": {
      "tags": ["scope:client"]
    },
    "client-feature-home": {
      "tags": ["scope:client"]
    },
    "extensions": {
      "tags": []
    },
    "server": {
      "tags": []
    },
    "ui": {
      "tags": ["scope:ui"]
    }
  }
}
